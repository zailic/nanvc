{
    "name": "nanvc",
    "description": "Not Another Node Vault Client :)",
    "version": "1.0.14",
    "author": {
        "name": "Ionut Zailic",
        "email": "ionut@zailic.com"
    },
    "contributors": [],
    "keywords": [
        "hashicorp",
        "vault",
        "consul",
        "aws",
        "secrets",
        "manage",
        "client"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/zailic/nanvc.git"
    },
    "homepage": "https://github.com/zailic/nanvc",
    "bugs": {
        "url": "https://github.com/zailic/nanvc/issues"
    },
    "license": "MIT",
    "main": "dist/main.js",
    "types": "dist/main.d.ts",
    "scripts": {
        "test:unit": "mocha -r ts-node/register test/unit/**/*.ts",
        "test:all": "mocha -r ts-node/register test/unit/**/*.ts test/integration/**/*.ts",
        "coverage": "nyc npm run test:all",
        "build": "rimraf ./dist/* && tsc",
        "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
        "tslint": "tslint -c ./tslint.json './src/**/*.ts'"
    },
    "nyc": {
        "include": [
            "src/**/*.ts",
            "src/**/*.tsx"
        ],
        "exclude": [
            "test/**/*.ts",
            "test/**/*.tsx"
        ],
        "extension": [
            ".ts",
            ".tsx"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "text-summary",
            "html",
            "lcov"
        ],
        "sourceMap": true,
        "instrument": true
    },
    "engines": {
        "node": ">=6"
    },
    "dependencies": {
        "request": "^2.87.0",
        "request-promise-native": "^1.0.5",
        "tv4": "^1.3.0",
        "@types/tv4": "^1.2.29",
        "@types/request-promise-native": "1.0.16"
    },
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/mocha": "^2.2.48",
        "@types/sinon": "^4.3.3",
        "chai": "^4.1.2",
        "coveralls": "^3.0.1",
        "mocha": "^5.2.0",
        "mocha-typescript": "^1.1.16",
        "nyc": "^11.9.0",
        "rimraf": "^2.6.2",
        "sinon": "^4.5.0",
        "ts-node": "^5.0.1",
        "tslint": "^5.10.0",
        "typescript": "^2.9.2"
    }
}
